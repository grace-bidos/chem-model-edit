{
  "name": "web",
  "projectType": "application",
  "sourceRoot": "apps/web/src",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C apps/web dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/apps/web/dist"],
      "options": {
        "command": "pnpm -C apps/web build"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C apps/web preview"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C apps/web lint"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C apps/web typecheck"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C apps/web format"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C apps/web test"
      }
    },
    "test-ui": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm -C apps/web test:ui"
      }
    }
  }
}
