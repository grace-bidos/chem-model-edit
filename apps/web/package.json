{
  "name": "chem-model-web",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --exclude src/**/*.a11y.test.* --exclude src/**/*.fastcheck.test.*",
    "test:ui": "vitest --ui",
    "test:a11y": "bash -lc 'shopt -s globstar; vitest run --passWithNoTests src/**/*.a11y.test.*'",
    "test:fastcheck": "bash -lc 'shopt -s globstar; vitest run --passWithNoTests src/**/*.fastcheck.test.*'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token $CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "check": "prettier --write . && eslint --fix",
    "deploy": "wrangler deploy",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@chem-model/api-client": "workspace:*",
    "@chem-model/shared": "workspace:*",
    "@faker-js/faker": "^10.0.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dockview": "^4.13.1",
    "dockview-core": "^4.13.1",
    "dockview-react": "^4.13.1",
    "lucide-react": "^0.561.0",
    "molstar": "^5.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6",
    "vite-tsconfig-paths": "^6.0.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.22.1",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-config": "^0.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "axe-core": "^4.11.1",
    "chromatic": "^12.2.0",
    "eslint": "^9.39.2",
    "eslint-plugin-tsdoc": "^0.4.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.5.3",
    "sass": "^1.97.1",
    "storybook": "^8.6.15",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.61.1"
  }
}
